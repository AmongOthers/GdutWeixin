@{
    ViewBag.Title = "About";
}

@section Header {
	<h1>@ViewBag.Title</h1>
}

<div data-role="collapsible-set">

    <div data-role="collapsible" data-collapsed="false">
        <h3>关于乐搜</h3>
        <p>
            <label>@ViewBag.About</label><br />
            <label>联系方式: </label><br />
            <label>@ViewBag.Contact</label>
        </p>
    </div>

    <div data-role="collapsible">
        <h3>功能简介</h3>
        <p>@ViewBag.Func</p>
    </div>

</div>